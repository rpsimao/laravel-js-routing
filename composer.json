{
    "name": "rpsimao/laravel-js-routing",
    "description": "Laravel 5 package to access Routing from Javascript",
    "license": "MIT",
    "keywords": ["laravel", "routing"],
    "authors": [
        {
            "name": "Jonathan DELORME",
            "email": "delorme.jonathan@gmail.com"
        }
    ],
    "require": {
        "php": ">=7.1.0"
    },
    "autoload": {
        "psr-0": {
            "rpsimao\\LaravelJsRouting\\": "src/"
        }
    },
    "minimum-stability": "stable",
    "extra": {
        "laravel": {
        "providers": [
            "rpsimao\\LaravelJsRouting\\LaravelJsRoutingServiceProvider"
        ],
        "aliases": {
            "JSRouter": "rpsimao\\LaravelJsRouting\\Facades\\JSRouter"
           }
        }
    }
}